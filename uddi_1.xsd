			<attribute name = "generic" minOccurs = "1" type = "string"/>
			<attribute name = "operator" minOccurs = "1" type = "string"/>
			<attribute name = "truncated">
				<datatype source = "NMTOKEN">
					<enumeration value = "true"/>
					<enumeration value = "false"/>
